{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": 2,
        "no-unused-vars": 0,
        "no-undef": 0,
        "no-redeclare": 0,
        "no-mixed-spaces-and-tabs": 0,
        "linebreak-style": 0
    }
}
